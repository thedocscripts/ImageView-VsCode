{
  "name": "imageview",
  "displayName": "imageview",
  "description": "Image Viewer on Hover over code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:html",
    "onLanguage:markdown",
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "extensions": [".js"],
        "aliases": ["JavaScript"]
      }
    ],
    "commands": [
      {
        "command": "imageview.downloadAndReplaceFromHover",
        "title": "Download and Replace Image URL"
      },
      {
        "command": "imageview.refreshImageLibrary",
        "title": "Refresh Image Library"
      },
      {
        "command": "vscode.open",
        "title": "Open File"
      }
    ],"viewsContainers": {
  "activitybar": [
    {
      "id": "imageview-sidebar",
      "title": "ImageView",
      "icon": "media/icon.png"
    }
  ]
},
"views": {
  "imageview-sidebar": [
    {
      "type": "webview",
      "id": "imageLibraryPanel",
      "name": "Image Library"
    }
  ]
}


    
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
